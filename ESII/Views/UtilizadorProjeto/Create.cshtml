@model ESII.Models.UtilizadorProjeto
@{
    ViewData["Title"] = "Adicionar Utilizador ao Projeto";
}

<h2>Adicionar Utilizador ao Projeto</h2>
<form asp-action="Create">
    <div class="form-group">
        <label asp-for="UtilizadorId">Utilizador ID</label>
        <input asp-for="UtilizadorId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ProjetoId">Projeto ID</label>
        <input asp-for="ProjetoId" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Adicionar</button>
</form>
<a asp-action="Index">Voltar para a Lista</a>
